<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" 
         version="2.5">
   <filter>
   	<filter-name>ObjectifyFilter</filter-name>
    	<filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
  </filter>
  <filter-mapping>
    	<filter-name>ObjectifyFilter</filter-name>
    	<url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>CreateEmoji</servlet-name>
    <servlet-class>es.upm.dit.isst.sp2.CreateEmoji</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>InsertEmoji</servlet-name>
    <servlet-class>es.upm.dit.isst.sp2.InsertEmoji</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ListEmojis</servlet-name>
    <servlet-class>es.upm.dit.isst.sp2.ListEmojis</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EmojisUser</servlet-name>
    <servlet-class>es.upm.dit.isst.sp2.EmojisUser</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>FindUsrEmojis</servlet-name>
    <servlet-class>es.upm.dit.isst.sp2.FindUsrEmojis</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DelUsrEmoji</servlet-name>
    <servlet-class>es.upm.dit.isst.sp2.DelUsrEmoji</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CreateEmoji</servlet-name>
    <url-pattern>/create</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>InsertEmoji</servlet-name>
    <url-pattern>/insert</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ListEmojis</servlet-name>
    <url-pattern>/list</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EmojisUser</servlet-name>
    <url-pattern>/usremojis</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FindUsrEmojis</servlet-name>
    <url-pattern>/lstuser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DelUsrEmoji</servlet-name>
    <url-pattern>/delemoj</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>